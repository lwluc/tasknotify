{
	"name": "tasknotify",
	"displayName": "TaskNotify",
	"description": "Get notifications when you vscode task is done",
	"version": "0.0.2",
	"publisher": "lwluc",
	"engines": {
		"vscode": "^1.47.0"
	},
	"icon": "assets/logo/tasknotify-icon.png",
	"galleryBanner": {
		"color": "#AFC0A5",
		"theme": "dark"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/lwluc/tasknotify.git"
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/vscode": "^1.47.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"eslint": "^7.6.0",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"glob": "^7.1.6",
		"mocha": "^8.0.1",
		"typescript": "^3.8.3"
	}
}